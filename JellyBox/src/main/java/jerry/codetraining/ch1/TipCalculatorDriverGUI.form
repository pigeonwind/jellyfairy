          </grid>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties/>
      </component>
      <component id="963de" class="javax.swing.JLabel" binding="billAmount">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="9" fill="0" indent="0" use-parent-layout="false"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="BILL AMOUNT"/>
          <verticalAlignment value="0"/>
          <verticalTextPosition value="0"/>
